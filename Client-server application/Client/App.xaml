<unity:PrismApplication x:Class="Client.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Client"
             xmlns:views="clr-namespace:Client.Views"
             xmlns:viewmodels="clr-namespace:Client.ViewModels"
             xmlns:unity="http://prismlibrary.com/">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Themes/ResourceDictionaries/Buttons.xaml"/>
                <ResourceDictionary Source="Themes/ResourceDictionaries/CheckBoxes.xaml"/>
                <ResourceDictionary Source="Themes/ResourceDictionaries/ListBoxes.xaml"/>
                <ResourceDictionary Source="Themes/ResourceDictionaries/TextBlocks.xaml"/>
                <ResourceDictionary Source="Themes/ResourceDictionaries/TextBoxes.xaml"/>
                <ResourceDictionary Source="Themes/ResourceDictionaries/Themes.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</unity:PrismApplication>
